<!--index.wxml-->
<view class="container">
  <view class="result-wrapper">
  	<image class="absolute-center-X img-large-size" src="{{resultImg}}"></image>
  	<text>恭喜您！成功预订车票！</text>
  </view>
	
  <view class="default-btn" wx:if="{{orderDetail.orderStatus == 1020101}}" bindtap="goPay">继续支付</view>
  <view class="default-btn" bindtap="orderDetail">订单详情</view>

</view>
